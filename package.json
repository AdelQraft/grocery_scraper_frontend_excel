{
	"name": "excel-custom-functions",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/OfficeDev/Excel-Custom-Functions.git"
	},
	"license": "MIT",
	"scripts": {
		"generate-manifest": "node build_utility/configuration.js manifest.xml.in .manifest_production_generated.xml -p",
		"generate-manifest:dev": "node build_utility/configuration.js manifest.xml.in .manifest_debug_generated.xml",
		"build": "webpack --mode production",
		"build:dev": "webpack --mode development",
		"dev-server": "webpack serve --mode development",
		"lint": "office-addin-lint check",
		"lint:fix": "office-addin-lint fix",
		"prestart": "npm run build",
		"prettier": "office-addin-lint prettier",
		"signin": "office-addin-dev-settings m365-account login",
		"signout": "office-addin-dev-settings m365-account logout",
		"start": "npm run generate-manifest:dev && office-addin-debugging start .manifest_debug_generated.xml",
		"start:desktop": "npm run generate-manifest:dev && office-addin-debugging start .manifest_debug_generated.xml desktop",
		"start:web": "npm run generate-manifest:dev && office-addin-debugging start .manifest_debug_generated.xml web",
		"stop": "npm run generate-manifest:dev && office-addin-debugging stop .manifest_debug_generated.xml",
		"validate": "npm run generate-manifest && office-addin-manifest validate .manifest_production_generated.xml",
		"validate:dev": "npm run generate-manifest:dev && office-addin-manifest validate .manifest_debug_generated.xml",
		"watch": "webpack --mode development --watch"
	},
	"dependencies": {
		"core-js": "^3.36.0",
		"regenerator-runtime": "^0.14.1"
	},
	"devDependencies": {
		"@babel/core": "^7.24.0",
		"@babel/preset-env": "^7.25.4",
		"@babel/preset-typescript": "^7.23.3",
		"@types/custom-functions-runtime": "^1.6.8",
		"@types/office-js": "^1.0.377",
		"@types/office-runtime": "^1.0.35",
		"acorn": "^8.5.0",
		"babel-loader": "^9.1.3",
		"copy-webpack-plugin": "^12.0.2",
		"custom-functions-metadata-plugin": "^1.7.2",
		"dotenv": "^16.4.7",
		"dotenv-webpack": "^8.1.0",
		"eslint-plugin-office-addins": "^3.0.3",
		"file-loader": "^6.2.0",
		"html-loader": "^5.0.0",
		"html-webpack-plugin": "^5.6.0",
		"office-addin-cli": "^1.6.5",
		"office-addin-debugging": "^5.1.6",
		"office-addin-dev-certs": "^1.13.5",
		"office-addin-lint": "^2.3.5",
		"office-addin-manifest": "^1.13.6",
		"office-addin-prettier-config": "^1.2.1",
		"os-browserify": "^0.3.0",
		"process": "^0.11.10",
		"request": "^2.88.2",
		"source-map-loader": "^5.0.0",
		"typescript": "^5.4.2",
		"webpack": "^5.95.0",
		"webpack-cli": "^5.1.4",
		"webpack-dev-server": "5.1.0"
	},
	"prettier": "office-addin-prettier-config",
	"browserslist": [
		"last 2 versions",
		"ie 11"
	]
}
